<% if developer_signed_in? %>
  <br>
  <h1><%= @who %></h1>
  Signed in as <%= current_developer.name %>
  <% if current_developer.admin? %>
  <ul>
    <% @developers.each do |developer| %>
      <li><%= link_to developer.name, developer_path(developer) %></li>
    <% end %>
  </ul>
  <% end %>  
  <% else %>
<% end %>

